{
  "name": "read-vue",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist dist_html _dist _package www",
    "see": "npm run clean && cross-env NODE_ENV=test atool-build --devtool none --no-compress",
    "dev": "npm run clean && cross-env NODE_ENV=development dora --plugins webpack,@alipay/dora-plugin-domain -p 9000",
    "build": "npm run clean && cross-env NODE_ENV=production atool-build",
    "anyproxy": "node ./mock/anyproxy",
    "mock": "npm run clean && cross-env NODE_ENV=development MOCK=true dora --plugins webpack,@alipay/dora-plugin-luna-mock?ignoreFiles=['common.js'],@alipay/dora-plugin-domain -p 9000",
    "qtscp": "npm run clean && npm run see && qt scp dist zmxy/read-vue",
    "pushToBridge": "sh ./script/pushToBridge.sh",
    "pullFromBridge": "sh ./script/pullFromBridge.sh",
    "moggles": "cross-env NODE_ENV=production moggles -o www --h5build portal",
    "moggles-mock": "cross-env NODE_ENV=development MOCK=true moggles -o www --h5build portal",
    "site": "site build www -o _package/site --h5app",
    "h5build": "npm run clean && npm run moggles && npm run site",
    "lintjs": "eslint \"src/**/*.js\"",
    "formatjs": "prettier-eslint \"src/**/*.js\" --write",
    "lintcss": "stylelint \"src/**/*.css\"",
    "formatcss": "stylelint \"src/**/*.css\" --fix",
    "-precommit": "lint-staged",
    "-commitmsg": "commitlint -e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nobody-nn/read-vue.git"
  },
